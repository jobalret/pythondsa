function cirTraversal(arr,N) {

    stringing=''
     for(var i =arr.length-1;i>=1;i--){
        string+=arr[i][0]+" "
    }
      
for(var i =0;i<N;i++){
    string+=arr[0][i]+" "
}

for(var i =1;i<N;i++){
    string+=arr[i][N-1]+" "
}

for(var i=N-2;i>0;i--){
    string+=arr[N-1][i]+" "
}



  console.log(string)

}
